-- special values
dqlogb820  logb   Infinity ->   Infinity
dqlogb821  logb   0        ->  -Infinity Division_by_zero
dqlogb822  logb   NaN      ->   NaN
dqlogb823  logb   sNaN     ->   NaN     Invalid_operation
-- propagating NaNs
dqlogb824  logb   sNaN123  ->   NaN123  Invalid_operation
