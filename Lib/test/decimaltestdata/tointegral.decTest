-- propagating NaNs
intx152 tointegral   NaN808    ->  NaN808
intx153 tointegral  sNaN080    ->  NaN80  Invalid_operation
intx154 tointegral  -NaN808    -> -NaN808
intx155 tointegral -sNaN080    -> -NaN80  Invalid_operation
