version: 2.39

-- This set of testcases tests raising numbers to an integer power only.
-- If arbitrary powers were supported, 1 ulp differences would be
-- permitted.

extended:    1
precision:   9
rounding:    half_up
maxExponent: 999
minexponent: -999

