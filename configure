# check tzset(3) exists and works like we expect it to
{ echo "$as_me:$LINENO: checking for working tzset()" >&5
echo $ECHO_N "checking for working tzset()... $ECHO_C" >&6; }
if test "${ac_cv_working_tzset+set}" = set; then
