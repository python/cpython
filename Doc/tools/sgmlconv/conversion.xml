  <macro name="chapter"
         closes="chapter chapter* section section* subsection subsection*
                 subsubsection subsubsection*
                 paragraph paragraph* subparagraph subparagraph*">
    <text>
</text>
    <child name="title"/>
    <content implied="yes"/>
    </macro>
